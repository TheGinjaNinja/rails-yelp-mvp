  <h1>Restaurants</h1>
  <br>
    <% @restaurants.each do |restaurant| %>
      <ul>
        <li> <%= link_to restaurant.name, restaurant_path(restaurant) %> | <%= restaurant.category %></li>
        <br>
        <%= restaurant.address %> <%= restaurant.phone_number %>
      </ul>
    <% end %>
<%= link_to 'Add a new restaurant', new_restaurant_path %>
